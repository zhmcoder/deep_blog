{
    "name": "andruby/deep-blog",
    "description": "简单的blog，首页使用grid的瀑布流实现展示，内容使用的markdown进行展示。",
    "license": "MIT",
    "authors": [
        {
            "name": "Andruby",
            "email": "322zx@163.com"
        }
    ],
    "homepage": "",
    "keywords": [
        "Laravel",
        "Admin",
        "Vue"
    ],
    "require": {
        "illuminate/support": "~5|~6|~7",
        "andruby/deep-admin": "^2.0.0",
        "binarytorch/larecipe": "^2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Andruby\\DeepBlog\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Andruby\\DeepBlog\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Andruby\\DeepBlog\\BlogServiceProvider"
            ]
        }
    },
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://mirrors.aliyun.com/composer/"
        }
    }
}
